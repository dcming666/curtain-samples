{"ast":null,"code":"import { onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport { useCurtainStore } from '../stores';\nexport default {\n  name: 'CurtainDetail',\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const curtainStore = useCurtainStore();\n    onMounted(async () => {\n      const curtainId = route.params.id;\n      if (curtainId) {\n        await curtainStore.fetchCurtainById(curtainId);\n      }\n    });\n    const goBack = () => {\n      router.back();\n    };\n    return {\n      curtainStore,\n      goBack\n    };\n  }\n};","map":{"version":3,"names":["onMounted","useRoute","useRouter","useCurtainStore","name","setup","route","router","curtainStore","curtainId","params","id","fetchCurtainById","goBack","back"],"sources":["C:\\Users\\Firebat\\Desktop\\curtain\\frontend\\frontend-app\\src\\views\\CurtainDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"detail-container\">\r\n    <div v-if=\"curtainStore.loading\" class=\"loading-container\">\r\n      <el-skeleton :rows=\"10\" animated />\r\n    </div>\r\n    <div v-else-if=\"curtainStore.error\" class=\"error-message\">\r\n      <el-alert :title=\"curtainStore.error\" type=\"error\" show-icon />\r\n      <el-button class=\"mt-4\" type=\"primary\" @click=\"goBack\">返回上一页</el-button>\r\n    </div>\r\n    <div v-else-if=\"curtainStore.currentCurtain\" class=\"curtain-detail\">\r\n      <el-row :gutter=\"30\">\r\n        <!-- 返回按钮 -->\r\n        <el-col :span=\"24\" class=\"mb-4\">\r\n          <el-button @click=\"goBack\" icon=\"el-icon-arrow-left\">返回</el-button>\r\n        </el-col>\r\n        \r\n        <!-- 左侧图片区域 -->\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"10\" :lg=\"8\">\r\n          <div class=\"image-container\">\r\n            <el-image \r\n              :src=\"curtainStore.currentCurtain.image_url\" \r\n              fit=\"contain\"\r\n              :preview-src-list=\"[curtainStore.currentCurtain.image_url]\"\r\n              class=\"curtain-image\"\r\n            >\r\n              <template #error>\r\n                <div class=\"image-error\">\r\n                  <el-icon><picture-failed /></el-icon>\r\n                  <span>加载图片失败</span>\r\n                </div>\r\n              </template>\r\n            </el-image>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <!-- 右侧信息区域 -->\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"14\" :lg=\"16\">\r\n          <div class=\"curtain-info\">\r\n            <h1 class=\"curtain-title\">{{ curtainStore.currentCurtain.name }}</h1>\r\n            \r\n            <div class=\"curtain-meta\">\r\n              <el-tag size=\"medium\" type=\"info\">{{ curtainStore.currentCurtain.category_name }}</el-tag>\r\n              <el-tag size=\"medium\" type=\"success\" v-if=\"curtainStore.currentCurtain.in_stock\">有库存</el-tag>\r\n              <el-tag size=\"medium\" type=\"danger\" v-else>缺货</el-tag>\r\n              <el-tag size=\"medium\" type=\"warning\" v-if=\"curtainStore.currentCurtain.is_new\">新品</el-tag>\r\n            </div>\r\n            \r\n            <div class=\"curtain-price\" v-if=\"curtainStore.currentCurtain.price\">\r\n              <span class=\"price-label\">价格:</span>\r\n              <span class=\"price-value\">¥{{ curtainStore.currentCurtain.price.toFixed(2) }}</span>\r\n              <span class=\"price-unit\">/米</span>\r\n            </div>\r\n            \r\n            <div class=\"curtain-specs\">\r\n              <h3>产品规格</h3>\r\n              <el-descriptions :column=\"1\" border>\r\n                <el-descriptions-item label=\"材质\">{{ curtainStore.currentCurtain.material || '未提供' }}</el-descriptions-item>\r\n                <el-descriptions-item label=\"宽度\">{{ curtainStore.currentCurtain.width || '未提供' }}</el-descriptions-item>\r\n                <el-descriptions-item label=\"图案\">{{ curtainStore.currentCurtain.pattern || '未提供' }}</el-descriptions-item>\r\n                <el-descriptions-item label=\"风格\">{{ curtainStore.currentCurtain.style || '未提供' }}</el-descriptions-item>\r\n                <el-descriptions-item label=\"功能\">{{ curtainStore.currentCurtain.features || '未提供' }}</el-descriptions-item>\r\n              </el-descriptions>\r\n            </div>\r\n            \r\n            <div class=\"curtain-description\">\r\n              <h3>产品描述</h3>\r\n              <p>{{ curtainStore.currentCurtain.description }}</p>\r\n            </div>\r\n            \r\n            <div class=\"contact-info\">\r\n              <h3>联系方式</h3>\r\n              <p>如果您对这款窗帘样本感兴趣，请联系我们:</p>\r\n              <p><el-icon><phone /></el-icon> 电话: 123-4567-8910</p>\r\n              <p><el-icon><message /></el-icon> 邮箱: contact@curtainshop.com</p>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n    <div v-else class=\"not-found\">\r\n      <el-empty description=\"未找到窗帘样本信息\" />\r\n      <el-button class=\"mt-4\" type=\"primary\" @click=\"goBack\">返回上一页</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted } from 'vue'\r\nimport { useRoute, useRouter } from 'vue-router'\r\nimport { useCurtainStore } from '../stores'\r\n\r\nexport default {\r\n  name: 'CurtainDetail',\r\n  setup() {\r\n    const route = useRoute()\r\n    const router = useRouter()\r\n    const curtainStore = useCurtainStore()\r\n    \r\n    onMounted(async () => {\r\n      const curtainId = route.params.id\r\n      if (curtainId) {\r\n        await curtainStore.fetchCurtainById(curtainId)\r\n      }\r\n    })\r\n    \r\n    const goBack = () => {\r\n      router.back()\r\n    }\r\n    \r\n    return {\r\n      curtainStore,\r\n      goBack\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.detail-container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.loading-container, .error-message, .not-found {\r\n  padding: 40px 0;\r\n  text-align: center;\r\n}\r\n\r\n.image-container {\r\n  background-color: #f5f7fa;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.curtain-image {\r\n  width: 100%;\r\n  height: 400px;\r\n  object-fit: contain;\r\n}\r\n\r\n.image-error {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 400px;\r\n  color: #909399;\r\n}\r\n\r\n.curtain-title {\r\n  font-size: 2rem;\r\n  color: #2c3e50;\r\n  margin: 0 0 15px 0;\r\n}\r\n\r\n.curtain-meta {\r\n  display: flex;\r\n  gap: 10px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.curtain-price {\r\n  margin: 20px 0;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.price-label {\r\n  color: #606266;\r\n}\r\n\r\n.price-value {\r\n  color: #f56c6c;\r\n  font-size: 1.8rem;\r\n  font-weight: bold;\r\n  margin: 0 5px;\r\n}\r\n\r\n.price-unit {\r\n  color: #909399;\r\n}\r\n\r\n.curtain-specs, .curtain-description, .contact-info {\r\n  margin-top: 30px;\r\n}\r\n\r\n.curtain-specs h3, .curtain-description h3, .contact-info h3 {\r\n  font-size: 1.3rem;\r\n  color: #2c3e50;\r\n  margin-bottom: 15px;\r\n  border-bottom: 1px solid #ebeef5;\r\n  padding-bottom: 10px;\r\n}\r\n\r\n.curtain-description p {\r\n  line-height: 1.6;\r\n  color: #606266;\r\n}\r\n\r\n.contact-info p {\r\n  margin: 10px 0;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.mb-4 {\r\n  margin-bottom: 16px;\r\n}\r\n\r\n.mt-4 {\r\n  margin-top: 16px;\r\n}\r\n</style>"],"mappings":"AAuFA,SAASA,SAAQ,QAAS,KAAI;AAC9B,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAW;AAC/C,SAASC,eAAc,QAAS,WAAU;AAE1C,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;IACvB,MAAMM,MAAK,GAAIL,SAAS,CAAC;IACzB,MAAMM,YAAW,GAAIL,eAAe,CAAC;IAErCH,SAAS,CAAC,YAAY;MACpB,MAAMS,SAAQ,GAAIH,KAAK,CAACI,MAAM,CAACC,EAAC;MAChC,IAAIF,SAAS,EAAE;QACb,MAAMD,YAAY,CAACI,gBAAgB,CAACH,SAAS;MAC/C;IACF,CAAC;IAED,MAAMI,MAAK,GAAIA,CAAA,KAAM;MACnBN,MAAM,CAACO,IAAI,CAAC;IACd;IAEA,OAAO;MACLN,YAAY;MACZK;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}